<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>pdbonly</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <Optimize>true</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DynamicData" Version="6.9.1.2588" />
    <PackageReference Include="Fody" Version="5.0.5">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="ReactiveUI" Version="9.14.3" />
    <PackageReference Include="ReactiveUI.Fody" Version="9.14.3" />
    <PackageReference Include="ReactiveUI.XamForms" Version="9.14.3" />
    <PackageReference Include="Splat" Version="7.1.1" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.1.0.44" />
    <PackageReference Include="Syncfusion.Xamarin.Core" Version="17.1.0.44" />
    <PackageReference Include="Syncfusion.Xamarin.SfChart" Version="17.1.0.44" />
    <PackageReference Include="System.Reactive" Version="4.1.5" />
    <PackageReference Include="System.Threading.Tasks.Extensions" Version="4.5.2" />
    <PackageReference Include="Xamarin.Essentials" Version="1.1.0" />
    <PackageReference Include="Xamarin.Forms" Version="3.6.0.344457" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\AppTheme.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\CybersicknessPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\BioDataChartPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\BioDataPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Models\" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Views\BioDataPage.xaml.cs">
      <DependentUpon>BioDataPage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\BioDataChartPage.xaml.cs">
      <DependentUpon>BioDataChartPage.xaml</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>